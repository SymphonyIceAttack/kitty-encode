{
  "name": "kitty-encode",
  "version": "1.0.0",
  "description": "Free online developer tools for encoding, hashing, and data transformation. Support for URL encoding, Base64, MD5, UUID generation, password generation, and character encoding conversion.",
  "keywords": [
    "developer-tools",
    "encoding",
    "base64",
    "md5",
    "uuid",
    "url-encoder",
    "password-generator",
    "encoding-converter",
    "online-tools",
    "web-tools",
    "data-transformation",
    "cryptography",
    "security-tools"
  ],
  "author": {
    "name": "SymphonyIceAttack",
